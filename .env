# S3270 command-line options (.env overrides).
# Values are passed to the s3270 CLI unless empty/false.

ALLOW_LOG_ACCESS=true

# --help: Display basic command-line help and then exit (default: false)
S3270_HELP=false

# -4: Prefer IPv4 addresses (default: false)
S3270_PREFER_IPV4=false

# -6: Prefer IPv6 addresses (default: false)
S3270_PREFER_IPV6=false

# -accepthostname: Name to match in the host's TLS certificate (default: (none))
S3270_ACCEPT_HOSTNAME=

# -cadir: Directory containing CA root certificates for TLS (default: (none))
S3270_CA_DIR=

# -cafile: File containing CA root certificate for TLS (default: (none))
S3270_CA_FILE=

# -callback: Connect to the specified port for an s3270 protocol session (default: (none))
S3270_CALLBACK=

# -certfile: File containing client certificate for TLS (default: (none))
S3270_CERT_FILE=

# -certfiletype: Type of client certificate file (pem or asn1) for TLS (default: pem)
S3270_CERT_FILE_TYPE=pem

# -chainfile: File containing chain of CA certificates for TLS (default: (none))
S3270_CHAIN_FILE=

# -clear: Set a Boolean resource to false (default: (none))
S3270_CLEAR=

# -clientcert: Name of client certificate for TLS (default: (none))
S3270_CLIENT_CERT=

# -codepage: Host EBCDIC code page (default: bracket)
S3270_CODE_PAGE=bracket

# -connecttimeout: Timeout before giving up on a host connection (default: (none))
S3270_CONNECT_TIMEOUT=

# -cookiefile: Specifies the security cookie file (default: (none))
S3270_COOKIE_FILE=

# -devname: Device name (workstation ID) response to TELNET NEW-ENVIRON request (default: (none))
S3270_DEV_NAME=

# -e: Command to run instead of connecting to a host (default: (none))
S3270_EXEC_COMMAND=

# -httpd: Start HTTP server (default: (none))
S3270_HTTPD=

# -keyfile: Key file for TLS client certificate (default: (none))
S3270_KEY_FILE=

# -keyfiletype: Type of client certificate key file (pem or asn1) for TLS (default: pem)
S3270_KEY_FILE_TYPE=pem

# -keypasswd: Password for the key file (OpenSSL) or certificate file (macOS) for TLS (default: (none))
S3270_KEY_PASSWORD=

# -loginmacro: Actions to run when host connection is established (default: (none))
S3270_LOGIN_MACRO=

# -minversion: Minimum version number required (major.minor type iteration) (default: (none))
S3270_MIN_VERSION=

# -model: Model of 3270 to emulate (default: 3279-4-E)
S3270_MODEL=3279-2-E

# -noverifycert: Do not verify the TLS host certificate (default: true)
S3270_NO_VERIFY_CERT=false

# -nvt: Force NVT mode -- do not negotiate 3270 mode (default: false)
S3270_NVT=false

# -oversize: Make the display larger than the default for the 3270 model (default: (none))
S3270_OVERSIZE=

# -port: TCP port to connect to (default: 23)
S3270_PORT=23

# -proxy: Type of proxy and proxy server to use (default: (none))
S3270_PROXY=

# -scriptport: Accept TCP connections for s3270 protocol sessions (default: (none))
S3270_SCRIPT_PORT=

# -scriptportonce: Accept only one -scriptport session (default: false)
S3270_SCRIPT_PORT_ONCE=false

# -set: Set a Boolean resource to true or set a resource to some value (default: (none))
S3270_SET=

# -socket: Accept s3270 protocol sessions on the Unix-domain socket /tmp/x3sck.PID (default: false)
S3270_SOCKET=false

# -tlsmaxprotocol: Set the maximum TLS protocol (default: varies)
S3270_TLS_MAX_PROTOCOL=

# -tlsminprotocol: Set the minimum TLS protocol (default: varies)
S3270_TLS_MIN_PROTOCOL=

# -tn: Use terminal-name as the terminal name reported to the host (default: varies)
S3270_TERMINAL_NAME=

# -trace: Turn on data stream and action tracing (default: false)
S3270_TRACE=false

# -tracefile: Use file-name as the file for data stream and action tracing (default: POSIX: /tmp/x3trc.PID; Windows: x3trc.PID.txt)
S3270_TRACE_FILE=

# -tracefilesize: Limit trace files to bytes bytes in size (default: (none))
S3270_TRACE_FILE_SIZE=

# -user: Use user-name in the reply to TELNET NEW-ENVIRON sub-negotiation (default: varies)
S3270_USER=

# -utenv: Allow unit-test-specific environment variables to have effect (default: false)
S3270_UTENV=false

# -utf8: Use UTF-8 encoding for all I/O on the workstation (default: false)
S3270_UTF8=false

# -v: Display version information on standard output and then exit (default: false)
S3270_VERSION=false

# -xrm: Set the value of a resource (default: (none))
S3270_XRM=

APP_USE_KEYPAD=false
